{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "lib": [
      "es6",
      "es2022"
    ],
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "typeRoots": [
      "./src/@types",
      "src/@types",
      "node_modules/@types"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "../node_modules", "dist", "upload"]
}
